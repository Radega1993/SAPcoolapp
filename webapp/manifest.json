{
	"_version": "1.8.0",
	"sap.app": {
		"id": "coolapp1.coolapp1",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "coolapp1.coolapp1.view.index",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "coolapp1.coolapp1.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "coolapp1.coolapp1.view",
				"controlAggregation": "pages",
				"controlId": "pageContainer"
			},
			"routes": [
				{
					"name": "home",
					"pattern": "",
					"target": [
						"home"
					]
				},
					{
					"name": "course",
					"pattern": "course",
					"target": [
						"course"
					]
				},
					{
					"name": "career",
					"pattern": "career",
					"target": [
						"career"
					]
				},
					{
					"name": "jobprofile",
					"pattern": "jobprofile",
					"target": [
						"jobprofile"
					]
				}
			],
			"targets": {
				"home": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Home"
				},
				"course": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "course"
				},
				"career": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "career"
				},
				"jobprofile": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "jobprofile"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}